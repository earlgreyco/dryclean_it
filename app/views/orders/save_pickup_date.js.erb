$('.pickup-<%= @order.id %>').html("<%= DateTime.now.in_time_zone('Pacific Time (US & Canada)').strftime('%D') %><br><%= DateTime.now.in_time_zone('Pacific Time (US & Canada)').strftime('%I:%M%p') %>");
<% @pickup = false %>
<% @order.customer.orders.each do |order| %>
	<% if order.picked_up == false %>
		<% @pickup = true %>
	<% end %>
<% end %>

<% if @pickup == false %>
	$('.customer-<%= @order.customer.id %>').html("<div class='customer-img'></div>");
<% end %>